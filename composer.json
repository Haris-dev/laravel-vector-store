{
  "name": "adrianmtanase/laravel-vector-store",
  "description": "Vector store abstraction layer for the Laravel framework.",
  "license": "MIT",
  "keywords": [
    "adrian",
    "adrian tanase",
    "adrianmtanase",
    "laravel",
    "ai",
    "vector",
    "store",
    "weaviate",
    "php"
  ],
  "homepage": "https://github.com/adrianmtanase/laravel-vector-store",
  "support": {
    "issues": "https://github.com/adrianmtanase/laravel-vector-store/issues",
    "source": "https://github.com/adrianmtanase/laravel-vector-store"
  },
  "authors": [
    {
      "name": "Adrian Tanase"
    }
  ],
  "require": {
    "php": "^8.2",
    "probots-io/pinecone-php": "^0.0.3",
    "spatie/laravel-package-tools": "^1.14.0",
    "timkley/weaviate-php": "dev-main"
  },
  "require-dev": {
    "orchestra/testbench": "^8.18",
    "phpstan/phpstan": "^1.4.7",
    "phpunit/phpunit": "^10.5.3",
    "vlucas/phpdotenv": "^5.6@dev"
  },
  "autoload": {
    "psr-4": {
      "AdrianTanase\\VectorStore\\": "src"
    }
  },
  "autoload-dev"     : {
    "psr-4": {
      "AdrianTanase\\VectorStore\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AdrianTanase\\VectorStore\\VectorStoreServiceProvider"
      ]
    }
  },
  "scripts"          : {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev"
}
